{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "indent": [true, "tabs", 4],
        "quotemark": [true, "single"],
        "no-namespace": false,
        "interface-name": false,
        "no-reference": false,
        "member-access": [true, "no-public"],
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
    },
    "rulesDirectory": []
}